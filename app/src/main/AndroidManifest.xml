        <activity android:name="ink.wsm.wtgm.activity.SettingActivity" />
        <activity android:name="ink.wsm.wtgm.activity.MaterialActivity" />
        <activity android:name="ink.wsm.wtgm.activity.LoginActivity" />
        <activity android:name="ink.wsm.wtgm.activity.ChooseMaterialsActivity" />
        <activity android:name="ink.wsm.wtgm.activity.MainActivity" android:exported="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>